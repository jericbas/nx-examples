{
  "name": "docus",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "docus/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/docus",
        "main": "docus/src/index.ts",
        "tsConfig": "docus/tsconfig.lib.json",
        "assets": [
          "docus/*.md",
          {
            "input": "./docus/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./docus/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./docus",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./docus",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["docus/**/*.ts", "docus/package.json"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "docus/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
